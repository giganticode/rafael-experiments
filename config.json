{
  "arch": {
    "bidir": false,
    "drop": {
      "multiplier": 1,
      "out": 0.4,
      "oute": 0.1,
      "outh": 0.3,
      "outi": 0.65,
      "w": 0.5
    },
    "emb_sz": 512,
    "n_hid": 512,
    "n_layers": 1,
    "name": "gru",
    "out_bias": true,
    "tie_weights": true
  },
  "base_model": null,
  "bptt": 200,
  "bs": 32,
  "corpus": {
    "extensions": "java",
    "path": "/path/to/dataset"
  },
  "prep_function": {
    "callable": "bpe",
    "params": ["10k"],
    "options": {
      "no_com": true,
      "no_spaces": true,
      "no_str": false,
      "no_unicode": true,
      "max_str_length": 14
    }
  },
  "training": {
    "activation_regularization": {
      "alpha": 2,
      "beta": 1
    },
    "gradient_clip": 5.0,
    "optimizer": {
      "momentum": 0.0,
      "name": "sgd"
    },
    "schedule": {
      "init_lr": 0.1,
      "max_epochs": 20,
      "max_lr_reduction_times": 6,
      "mult_coeff": 0.5,
      "name": "rafael",
      "patience": 1
    },
    "sub_epochs": null,
    "weight_decay": 1.2e-06
  }
}
